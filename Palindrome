package test.test1;

class Solution{
	
	public static void main(String args[]) {
		Solution sol = new Solution();
		System.out.print(sol.isPali("madam"));
	}
	
	private boolean isPali(String str) {
		for(int i =0; i<=str.length()/2; i++) {
			if(str.charAt(i) != str.charAt(str.length()-i-1)) {
				return false;
				
			}					
		}
		return true;
	}
}
